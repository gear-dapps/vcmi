{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "gear-connector",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "window": {
        "startDragging": true
      }
    },
    "bundle": {
      "active": false,
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.tauri.dev1",
      "targets": "deb"
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "transparent": true,
        "decorations": false,
        "fullscreen": false,
        "center": true,
        "resizable": true,
        "title": "gear-online",
        "label": "lobby",
        "url": "lobby.html",
        "width": 1280,
        "height": 800
      },
      {
        "transparent": true,
        "decorations": false,
        "fullscreen": false,
        "center": false,
        "visible": false,
        "resizable": true,
        "title": "gear-online",
        "label": "log",
        "url": "connection_view.html"
      }
    ]
  }
}
